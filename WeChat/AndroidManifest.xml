            android:name="com.chatme.chat.controllor.Appstart"
            android:label="@string/title_activity_main"
            android:theme="@style/Anim_style2" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.chatme.chat.controllor.Regist" android:theme="@android:style/Theme.Black.NoTitleBar"/>
        <activity android:name="com.chatme.chat.controllor.WhatsnewDoor" android:theme="@style/Anim_style2" />
        <activity android:name="com.chatme.chat.controllor.MainChat" android:theme="@style/Anim_style2" />
